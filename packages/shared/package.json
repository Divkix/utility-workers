{
  "name": "@utility-workers/shared",
  "version": "1.0.0",
  "private": true,
  "description": "Shared types and utilities for utility workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pdf": {
      "types": "./dist/pdf-types.d.ts",
      "import": "./dist/pdf-types.js"
    },
    "./ai": {
      "types": "./dist/ai-types.d.ts",
      "import": "./dist/ai-types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
